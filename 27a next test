// 27a next test
#include <bits/stdc++.h>
#define optimize() ios_base ::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
#define ll long long
using namespace std;
int main()
{
    optimize();
    int n;
    cin >> n;

    int a[n];

    for (int i = 0; i < n; i++)
        cin >> a[i];

    sort(a, a + n);

    int ans = 1;
    for (int i = 0; i < n; i++)
    {
        if (ans == a[i])
        {
            ans++;
        }
    }

    cout << ans ;
}
