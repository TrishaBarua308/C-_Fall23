#include <bits/stdc++.h>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    string s;  getline(cin,s);
    
    int n= s.length();
    
    for(int i=0; i<n; i++)
    {
        if(s[i]==' ')
        {
            s[i]='\n';
        }
    }
    cout<<s;
      
    return 0;
}
