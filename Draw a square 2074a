#include<bits/stdc++.h>
using namespace std;
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
int main()
{
    optimize();
    int t;  cin>>t;

    while(t--)
    {
        int l,r,d,u;
        cin>>l>>r>>d>>u;

        if(l==r && r==d && d==u && l==u)  cout<<"YES"<<endl;
        else  cout<<"NO"<<endl;

    }
}
