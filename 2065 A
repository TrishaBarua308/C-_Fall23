#include <bits/stdc++.h>
#define endl '\n'
#define optimize()                 \
    ios_base ::sync_with_stdio(0); \
    cin.tie(0);                    \
    cout.tie(0);
using namespace std;
int main()
{
    optimize();
    int t;
    cin >> t;

    while (t--)
    {
        string s;
        cin >> s;
        int n = s.length();

        if (n <= 2)
            cout << "i" << endl;
        else
        {
            s[n - 2] = 'i';
            for (int i = 0; i < n - 1; i++)
            {
                cout << s[i];
            }
        }

        cout << endl;
    }
}
